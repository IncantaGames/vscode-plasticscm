{
  "rules": {
    "no-string-throw": true,
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "curly": true,
    "class-name": true,
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": true,
    "only-arrow-functions": [true, "allow-named-functions"],
    "no-unbound-method": true
  },
  "defaultSeverity": "warning"
}
