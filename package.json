{
  "name": "plastic-scm",
  "displayName": "Plastic SCM",
  "description": "",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Plastic SCM for vscode - Use 'PlasticSCM: Open Settings' to open settings in the IDE",
      "properties": {
        "plasticscm.cmConfiguration.cmPath": {
          "type": "string",
          "default": "",
          "markdownDescription": "Specifies a given cm.exe executable that is not in your `PATH` environment variable",
          "scope": "window"
        },
        "plasticscm.cmConfiguration.millisToWaitUntilUp": {
          "type": "number",
          "default": 5000,
          "markdownDescription": "Milliseconds to wait for the `cm` shell to be up. In some systems, shell initialization might be slow.",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
